          },
          {
            "Name": "errm-policy-triggered",
            "Properties": {
              "DefaultMessageTimeToLive": "PT12H",
              "MaxDeliveryCount": 3,
              "DeadLetteringOnMessageExpiration": true
            }
          }
        ]
      }
    ],
    "Logging": {
      "Type": "File"
    }
  }
}
